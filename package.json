{
  "name": "abrazar-api",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "bullmq": "^5.65.1",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.4",
    "yamljs": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "^5.0.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests",
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": [
        "babel-jest",
        {
          "configFile": "./babel.config.js"
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testRunner": "jest-circus/runner"
  }
}
